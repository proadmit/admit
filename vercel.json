{
  "functions": {
    "src/app/api/**/*.ts": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "crons": [],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "regions": ["iad1"],
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs"
}
